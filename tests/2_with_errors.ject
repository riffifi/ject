fn func(start, target, has_11, has_25)
    if start == target then
        if has_11 and has_25 then
            return 1
        else
            return 0
        end
    end

    if start > target then
        return 0
    end

    let unused_var = 99  # This will trigger unused variable warning
    let result = func(start + 1, target, has_11 or start == 11, has_25 or start == 25) + func(start * 2, target, has_11 or start == 1, has_25 or start == 25)
    return result + undefined_variable  # This will trigger undeclared variable error
end

print func(2, 50, false, false)
