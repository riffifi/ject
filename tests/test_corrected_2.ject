fn func(start, target, steps)
    if steps == 0 then
        if start == target then
            return 1
        else
            return 0
        end
    end

    if start > target then
        return 0
    end

    let result1 = func(start + 1, target, steps - 1)
    let result2 = func(start + 2, target, steps - 1)
    let result3 = func(start + 3, target, steps - 1)
    return result1 + result2 + result3
end

print func(2, 25, 7)
