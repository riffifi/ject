fn func(start, target, has_11=false, has_25=false)
    if start == target then
        if has_11 and has_25 then
            return 1
        else
            return 0
        end
    end

    if start > target then
        return 0
    end

    return func(start + 1, target, has_11 or start == 11, has_25 or start == 25) + 
    func(start * 2, target, has_11 or start == 1, has_25 or start == 25)
end

