let score = 85
let extra_credit = true

if score >= 90
    print "Base grade: A"
else if score >= 80
    print "Base grade: B"
    if extra_credit
        print "With extra credit: B+"
    else
        print "Final grade: B"
    end
else if score >= 70
    print "Base grade: C"
else
    print "Base grade: F"
end

print "Grading complete!"
