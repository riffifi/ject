# Create an initial array
let numbers = [1, 2, 3]
print "Initial array: " + numbers

# Test push function
let with_four = push(numbers, 4)
print "After push(numbers, 4): " + with_four

let with_hello = push(with_four, "hello")
print "After push(with_four, \"hello\"): " + with_hello

# Test push with different types
let mixed = push([1, "two"], true)
print "Mixed types push([1, \"two\"], true): " + mixed

# Test pop function
let original = [10, 20, 30, 40]
print "Original array: " + original

let popped = pop(original)
print "Popped value: " + popped
print "Original array after pop: " + [10, 20, 30]  # Original should be unchanged

