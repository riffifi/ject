{
  "name": "ject-linter",
  "displayName": "Ject Linter",
  "description": "VS Code linter for the Ject language.",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": ["Linters", "Programming Languages"],
  "activationEvents": [
    "onLanguage:ject"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ject",
        "aliases": ["Ject", "ject"],
        "extensions": [".ject"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ject",
        "scopeName": "source.ject",
        "path": "./syntaxes/ject.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}
