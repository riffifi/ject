# Simple showcase of Ject's expression features

let score = 85

# Conditional expressions
let grade = if score >= 90 then "A" elseif score >= 80 then "B" else "C" end
print "Grade: " + grade

# Match expressions
let color = "blue"
let color_code = match color
    "red" -> "#FF0000"
    "blue" -> "#0000FF"
    _ -> "#000000"
end
print "Color: " + color_code

# Combined usage
let message = if score >= 80 then "Great job!" else "Keep trying!" end
print "Message: " + message

print "Done!"
